{
  "expo": {
    "name": "Windel Vendas",
    "slug": "windel-app",
    "version": "1.0.0",
    "scheme": "windel-vendas",
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "orientation": "portrait",
    "icon": "./assets/windel_icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/windel.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.windelapp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.windelapp"
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "00578223-8d62-4ed6-bdd9-b4e231cc0f1b"
      }
    },
    "owner": "drigue_franc"
  }
}
